<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.brendanlim.wehaveweneed"
      android:versionCode="1"
      android:versionName="1"
>
	<uses-sdk android:minSdkVersion="3" />
	<application
    	android:icon="@drawable/appicon"
      	android:label="WeHaveWeNeed"
      	android:name="WehaveweneedApplication"
      	android:debuggable="false"
	>
    	<activity
        	android:name=".WehaveweneedActivity"
            android:label="WeHaveWeNeed"
            android:configChanges="keyboardHidden|orientation"
            android:launchMode="singleTask"
			android:theme="@style/Theme.Titanium"
        >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
		<activity
        	android:name="org.appcelerator.titanium.TitaniumActivity"
        	android:configChanges="keyboardHidden|orientation"
      	/>

		
		
<!--
		<provider
			android:authorities="com.brendanlim.wehaveweneed.titanium"
			android:name="org.appcelerator.titanium.TitaniumContentProvider"
		/>
-->
		<service
			android:name="org.appcelerator.titanium.TitaniumAnalyticsService"
			android:exported="false"
		/>
	</application>
	
	<uses-permission android:name="android.permission.INTERNET"/>
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
	
	
</manifest>
